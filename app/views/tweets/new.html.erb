<h1>邦ロックタイプ診断</h1>
<%= form_for @tweet do |f| %>

  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :title, :size => 30 %>
    <%= f.label :body %>
    <%= f.text_field :body, :size => 140 %>
  </div>

   <div class="field">
    <%= f.label :image %>
    <%= f.file_field :image %>
  </div>

  <%= f.submit "投稿する" %>
<% end %>

<%= link_to "一覧に戻る", tweets_path %>